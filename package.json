{
  "name": "chordial",
  "version": "0.1.0",
  "private": true,
  "main": "electron/dist/index.js",
  "scripts": {
    "serve:ui": "cross-env NODE_ENV=development vue-cli-service serve  --host localhost",
    "build:ui": "cross-env NODE_ENV=production vue-cli-service build",
    "serve:app": "cross-env NODE_ENV=development rm -rf electron/dist/ && tsc -p electron/src/tsconfig.json && electron .",
    "serve": "concurrently -k \"npm run serve:ui\" \"sleep 10 && npm run serve:app\"",
    "serve:cli": "npm run serve"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "primeflex": "^2.0.0",
    "primeicons": "^4.1.0",
    "primevue": "^3.0.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "electron": "^11.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "prettier": "^1.19.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "typescript": "^3.9.7"
  }
}
